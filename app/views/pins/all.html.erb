<% @user.each do |user| %>
    <div class = "panel">
        <% if user.occupation == "Teacher" %>
      <%= link_to user.firstname + user.lastname  , "teachers/#{user.username}" %>
        <% else %>
      <%= link_to user.firstname + user.lastname  , "users/#{user.username}" %>
          <% end %>
    </div><br>
<% end %>